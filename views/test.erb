<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>

  <head>
    <title>Test page containing all glyphs</title>
    <%= erb :header %>
  </head>
  <body>

    <% @glyphs.each do |glyph| %>
    <a href="/glyphs/<%= glyph %>">
    <div class="glyph" data-name="<%= glyph %>"></div>
    </a>
    <% end %>

  </body>
</html>
